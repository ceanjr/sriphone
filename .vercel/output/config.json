{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_image/?$",
			"dest": "_render"
		},
		{
			"src": "^/admin/analytics/?$",
			"dest": "_render"
		},
		{
			"src": "^/admin/banners/?$",
			"dest": "_render"
		},
		{
			"src": "^/admin/categorias/?$",
			"dest": "_render"
		},
		{
			"src": "^/admin/dashboard/?$",
			"dest": "_render"
		},
		{
			"src": "^/admin/login/?$",
			"dest": "_render"
		},
		{
			"src": "^/admin/produtos/?$",
			"dest": "_render"
		},
		{
			"src": "^/admin/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/admin/auth/login/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/admin/auth/logout/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/admin/categorias/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/admin/categorias/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/admin/produtos/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/admin/produtos/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/admin/upload/?$",
			"dest": "_render"
		},
		{
			"src": "^/catalogo/?$",
			"dest": "_render"
		},
		{
			"src": "^/$",
			"dest": "_render"
		}
	]
}